  "scripts": {
    "start": "concurrently \"devl -s rankings=http://localhost:3001 -s tournament=http://localhost:3001\" \"node ./dummy-modules.js\"",
    "build": "webpack -p",
    "watch": "webpack --watch",
    "prepack": "yarn run build",
    "test": "cross-env SECRET=mock_secret PORT=0000 mocha --recursive",
    "test:coverage": "nyc -n=lib/**/*.js --reporter=text yarn test",
    "test:ci": "nyc -n=lib/**/*.js --reporter=lcov yarn test && codecov",
    "lint": "eslint --ext .js ./server ./client",
    "lint:fix": "eslint --ext .js --fix ./",
    "generate": "node ./dev-scripts/generate.js"
  },
